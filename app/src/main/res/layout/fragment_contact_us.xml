        app:layout_constraintTop_toBottomOf="@+id/contact_num2"
        android:textSize="24dp"
        android:textColor="@color/black_myaccount"

        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"/>
    <TextView
        android:id="@+id/email_details"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Contact no."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email"
        android:textSize="24dp"
        android:textColor="@color/black_myaccount"

        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"/>
    <TextView
        android:id="@+id/grev_office"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Contact no."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_details"
        android:textSize="24dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"/>
    <TextView
        android:id="@+id/grev_office_details"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Contact no."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/grev_office"
        android:textSize="24dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"/>
    <TextView
        android:id="@+id/care"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Customer Care."
        app:layout_constraintEnd_toEndOf="parent"
